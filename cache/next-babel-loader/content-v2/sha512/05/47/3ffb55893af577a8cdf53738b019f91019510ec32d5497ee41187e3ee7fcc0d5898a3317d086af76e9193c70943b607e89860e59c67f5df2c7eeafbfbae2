{"ast":null,"code":"import axios from '../lib/axios.js';\nexport var getItems = function getItems(query) {\n  return new Promise(function (resolve, reject) {\n    var search = query.normalize('NFD').replace(/[\\u0300-\\u036f]/g, '');\n    axios.get(\"/api/items?q=\".concat(search)).then(function (res) {\n      resolve(res.data);\n    })[\"catch\"](function (err) {\n      if (err.response && err.response.data && err.response.data.message) {\n        reject(new Error(err.response.data.message));\n      } else {\n        reject(new Error('Error inesperado'));\n      }\n    });\n  });\n};\nexport var getItem = function getItem(productId) {\n  return new Promise(function (resolve, reject) {\n    axios.get(\"/api/items/\".concat(productId)).then(function (res) {\n      resolve(res.data.item);\n    })[\"catch\"](function (err) {\n      if (err.response && err.response.data && err.response.data.message) {\n        reject(new Error(err.response.data.message));\n      } else {\n        reject(new Error('Error inesperado'));\n      }\n    });\n  });\n};","map":null,"metadata":{},"sourceType":"module"}